{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@headlessui/react": "^2.0.4",
    "@mui/icons-material": "^5.15.20",
    "@mui/styled-engine": "^5.15.14",
    "axios": "^1.6.8",
    "lightweight-charts": "^4.1.6",
    "npm-check-updates": "^16.14.20",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.4.0",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.1",
    "styled-components": "^5.3.5",
    "webpack": "5.92.0",
    "webpack-cli": "^4.9.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^14.0.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-svg": "^5.0.0",
    "autoprefixer": "^10.4.19",
    "browser-sync": "^2.27.10",
    "chokidar-cli": "^3.0.0",
    "css-loader": "^7.1.2",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "file-loader": "^6.2.0",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "postcss-cli": "^11.0.0",
    "prettier": "^2.7.1",
    "react-scripts": "^5.0.1",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.3",
    "ts-loader": "^9.5.1",
    "url-loader": "^4.1.1"
  },
  "scripts": {
    "start": "npm-run-all --serial build watch:client",
    "build": "npm-run-all --serial build:css build:webpack ",
    "watch:client": "npm-run-all --parallel react-start watch:css watch:jsx",
    "watch:css": "postcss src/tailwind.css -o src/index.css --watch",
    "watch:jsx": "chokidar 'src/**/*.{js,jsx,ts,tsx}' -c 'npm run build'",
    "sync": "browser-sync start --proxy 'http://localhost:3000' --files 'src/**/*.css, src/**/*.js, src/**/*.jsx', src/**/*.ts, src/**/*.tsx''",
    "lint": "eslint 'src/**/*.{js,jsx, ts, tsx}' --quiet",
    "build:webpack": "webpack --config webpack.config.js --mode production",
    "build:css": "postcss src/tailwind.css -o src/index.css",
    "format": "prettier --write 'src/**/*.{js,jsx,css,scss,ts,tsx}'",
    "react-start": "react-scripts start",
    "react-test": "react-scripts test --env=jsdom",
    "react-eject": "react-scripts eject"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.{css,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
